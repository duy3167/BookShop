@{
    ViewData["Title"] = "Category";
    Layout = "_ManagementLayout";
}
@model Category
<form asp-controller="Category" asp-action="Edit" method="post" id="cate_form">
        <label for="txt_sup_name">Category name</label>
        <input type="hidden" asp-for="cate_id">
        <input type="text" asp-for="name" placeholder="category name" id="txt_cate_name" class="form-control mb-1" required>
        <button type="submit" class="btn btn-success">Update</button>
</form>